<div>
  <h1 class="font-bold text-4xl">OpenAI api key:</h1>
  <div class="p-2">
    <%= form_with model: @token do |form| %>
      <%= form.text_field :token %>
      <%= form.submit "Submit", class: "bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded" %>
    <% end %>
  </div>
</div>
